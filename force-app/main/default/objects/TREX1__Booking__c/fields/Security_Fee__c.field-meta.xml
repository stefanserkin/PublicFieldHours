<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Security_Fee__c</fullName>
    <description>The total rate for security guards for this booking</description>
    <externalId>false</externalId>
    <formula>IF( !ISBLANK( Number_of_Security_Hours__c ),
  Number_of_Security_Hours__c * Number_of_Security_Guards__c * Security_Hourly_Rate__c ,
  0
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The total rate for security guards for this booking</inlineHelpText>
    <label>Security Fee</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
