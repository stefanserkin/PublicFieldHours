<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Additional_Staff_Fee__c</fullName>
    <description>The total rate for additional staff for this booking</description>
    <externalId>false</externalId>
    <formula>IF( !ISBLANK( Number_of_Additional_Staff_Hours__c ),
  Number_of_Additional_Staff_Hours__c * Number_of_Additional_Staff__c * Additional_Staff_Hourly_Rate__c,
  0
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The total rate for additional staff for this booking</inlineHelpText>
    <label>Additional Staff Fee</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
